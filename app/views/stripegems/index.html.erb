<p id="notice"><%= notice %></p>

<h1>Stripegems</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stripegems.each do |stripegem| %>
      <tr>
        <td><%= link_to 'Show', stripegem %></td>
        <td><%= link_to 'Edit', edit_stripegem_path(stripegem) %></td>
        <td><%= link_to 'Destroy', stripegem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stripegem', new_stripegem_path %>
